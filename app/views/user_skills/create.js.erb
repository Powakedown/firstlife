$('#skill-<%= @skill.id %>').html('<%= j render "skills/skill", skill: @skill %>');

$('#title').removeClass("expandOpen");
window.requestAnimationFrame(function(time) {
window.requestAnimationFrame(function(time) {
    $('#title').addClass("expandOpen");
  });
  });

$('#title .skill-count').text('<%= current_user.skills.where(tree: @skill.tree).count %>');
